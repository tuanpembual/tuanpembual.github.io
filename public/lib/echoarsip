cp /etc/apt/sources.list /etc/apt/sources.list.ori
echo "## Repository BlankOn Suroboyo - Original" | tee /etc/apt/sources.list
echo "deb http://arsip.blankonlinux.or.id/blankon suroboyo main restricted extras extras-restricted" | tee -a /etc/apt/sources.list
echo "deb http://arsip.blankonlinux.or.id/blankon suroboyo-updates main restricted extras extras-restricted" | tee -a /etc/apt/sources.list
echo "deb http://arsip.blankonlinux.or.id/blankon suroboyo-security main restricted extras extras-restricted" | tee -a /etc/apt/sources.list